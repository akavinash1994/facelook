
 <%= link_to(new_user_profile_picture_path(@user)) do %>
 <div class="css-shapes-preview">
    <% @user.profile_pictures.each do |pic| %>
         
       <%= link_to image_tag(pic.image.url, class: 'media-object css-shapes-preview'), pic.image.url, target: '_blank' %>
      
    <% end %>

 </div>
<% end %>
 