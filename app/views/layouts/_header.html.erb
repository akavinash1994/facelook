<header class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
         <%= link_to "FaceLook", root_url, id: "logo" %> 
        <nav>
          <ul class="nav navbar-nav navbar-right">
            <li><%= link_to "Home", '#'%></li>
            <li><%= link_to "Help",   '#' %></li>
            <% if logged_in? %>
              <li><%= link_to "Users", '#' %></li>
              <li class="dropdown">
               <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Account <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "Profile", current_user %></li>
                <li><%= link_to "Edit", edit_user_path(current_user) %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "Log out", session_path(current_user), method: :delete %>
                </li>
            </ul>
          </li>
            <% else %>
             <%= link_to "LOGIN",new_session_path, class: "navbar-brand" %>
            <% end %>
          
          </ul>
        </nav>
      </div>
</header>