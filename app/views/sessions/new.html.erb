<h1>Login</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">  
    <%= form_for :session, url: sessions_path do |f| %>
      <%= f.label :email, "Email" %>
      <%= f.email_field :email %>

      <%= f.label :password, "Password" %>
      <%= f.password_field :password %>
      
      <%= f.submit "Login", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>